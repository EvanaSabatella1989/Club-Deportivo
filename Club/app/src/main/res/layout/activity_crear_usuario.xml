<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    >


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:layout_marginBottom="20dp"
                android:contentDescription="TODO"
                android:importantForAccessibility="no"
                android:orientation="horizontal"
                android:textAlignment="gravity"
                android:gravity="center"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="HardcodedText,MissingConstraints,UseCompoundDrawables">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="wrap_content"
                    android:layout_height="250dp"
                    android:adjustViewBounds="true"
                    android:importantForAccessibility="no"
                    android:src="@mipmap/icono_persona"
                    tools:srcCompat="@mipmap/icono_persona" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/input_background"
                android:contentDescription="TODO"
                android:importantForAccessibility="no"
                android:orientation="horizontal"
                android:paddingTop="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="HardcodedText,MissingConstraints,UseCompoundDrawables">

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="10sp"
                    android:text="@string/llave_nombre"
                    android:textSize="20sp"
                    tools:ignore="RtlSymmetry,TextSizeCheck" />

                <EditText
                    android:id="@+id/EditTextNombre"
                    android:layout_width="133dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="10sp"
                    android:hint="@string/sin_valor_nombre"
                    android:textColorHint="#B0B0B0"
                    android:textAlignment="gravity"
                    android:textSize="18sp"
                    android:minHeight="48dp"
                    tools:ignore="RtlHardcoded,RtlSymmetry,TextSizeCheck" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/input_background"
                android:contentDescription="TODO"
                android:importantForAccessibility="no"
                android:orientation="horizontal"
                android:paddingTop="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="HardcodedText,MissingConstraints,UseCompoundDrawables">

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:paddingLeft="10sp"
                    android:text="@string/llave_apellido"
                    android:textSize="20sp"
                    tools:ignore="RtlSymmetry,TextSizeCheck" />

                <EditText
                    android:id="@+id/EditTextApellido"
                    android:layout_width="158dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="10sp"
                    android:hint="@string/sin_valor_apellido"
                    android:textColorHint="#B0B0B0"
                    android:textAlignment="gravity"
                    android:textSize="18sp"
                    android:minHeight="48dp"
                    tools:ignore="RtlHardcoded,RtlSymmetry,TextSizeCheck" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/input_background"
                android:contentDescription="TODO"
                android:importantForAccessibility="no"
                android:orientation="horizontal"
                android:paddingTop="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="HardcodedText,MissingConstraints,UseCompoundDrawables">

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:paddingLeft="10sp"
                    android:text="@string/llave_dni"
                    android:textSize="20sp"
                    tools:ignore="RtlSymmetry,TextSizeCheck" />

                <EditText
                    android:id="@+id/EditTextDni"
                    android:layout_width="158dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="10sp"
                    android:hint="@string/sin_valor_dni"
                    android:textColorHint="#B0B0B0"
                    android:textAlignment="gravity"
                    android:textSize="18sp"
                    android:minHeight="48dp"
                    tools:ignore="RtlHardcoded,RtlSymmetry,TextSizeCheck" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/input_background"
                android:contentDescription="TODO"
                android:importantForAccessibility="no"
                android:orientation="horizontal"
                android:paddingTop="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="HardcodedText,MissingConstraints,UseCompoundDrawables">

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:paddingLeft="10sp"
                    android:text="@string/llave_socio"
                    android:textSize="20sp"
                    tools:ignore="RtlSymmetry,TextSizeCheck" />

                <CheckBox
                    android:id="@+id/CheckBoxIsSocio"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="end"
                    android:layout_marginEnd="10dp"
                    android:minWidth="48dp"
                    android:minHeight="48dp"
                    android:padding="10dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/input_background"
                android:contentDescription="TODO"
                android:importantForAccessibility="no"
                android:orientation="horizontal"
                android:paddingTop="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="HardcodedText,MissingConstraints,UseCompoundDrawables">

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="219dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:paddingLeft="10sp"
                    android:text="@string/llave_fechaNac"
                    android:textSize="20sp"
                    tools:ignore="RtlSymmetry,TextSizeCheck" />

                <EditText
                    android:id="@+id/EditTextFechaNac"
                    android:layout_width="158dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="10sp"
                    android:hint="@string/valor_fechaNac"
                    android:textColorHint="#B0B0B0"
                    android:textAlignment="gravity"
                    android:textSize="18sp"
                    android:minHeight="48dp"
                    android:inputType="number"
                    android:maxLength="10"
                    tools:ignore="RtlHardcoded,RtlSymmetry,TextSizeCheck" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/input_background"
                android:contentDescription="TODO"
                android:importantForAccessibility="no"
                android:orientation="horizontal"
                android:paddingTop="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="HardcodedText,MissingConstraints,UseCompoundDrawables">

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:paddingLeft="10sp"
                    android:text="@string/llave_domicilio"
                    android:textSize="20sp"
                    tools:ignore="RtlSymmetry,TextSizeCheck" />

                <EditText
                    android:id="@+id/EditTextDomicilio"
                    android:layout_width="158dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="10sp"
                    android:hint="@string/sin_valor_domicilio"
                    android:textColorHint="#B0B0B0"
                    android:textAlignment="gravity"
                    android:textSize="18sp"
                    android:minHeight="48dp"
                    tools:ignore="RtlHardcoded,RtlSymmetry,TextSizeCheck" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/input_background"
                android:contentDescription="TODO"
                android:importantForAccessibility="no"
                android:orientation="horizontal"
                android:paddingTop="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="HardcodedText,MissingConstraints,UseCompoundDrawables">

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:paddingLeft="10sp"
                    android:text="@string/llave_telefono"
                    android:textSize="20sp"
                    tools:ignore="RtlSymmetry,TextSizeCheck" />

                <EditText
                    android:id="@+id/EditTextTelefono"
                    android:layout_width="158dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingRight="10sp"
                    android:hint="@string/sin_valor_telefono"
                    android:textColorHint="#B0B0B0"
                    android:textAlignment="gravity"
                    android:textSize="18sp"
                    android:minHeight="48dp"
                    tools:ignore="RtlHardcoded,RtlSymmetry,TextSizeCheck" />
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:contentDescription="TODO"
                android:gravity="center"
                android:importantForAccessibility="no"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <Button
                    android:id="@+id/btnAgregarUsuario"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/boton_grande"
                    android:text="@string/boton_aceptar"
                    android:textColor="@color/white"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"

                />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>

